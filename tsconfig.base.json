  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "declaration": true,
    "emitDeclarationOnly": true,
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitAny": false,
  },
